<FindBugsFilter>
    <Match>
        <Class name="~.*Test$" />
        <Bug pattern="DM_DEFAULT_ENCODING" />
    </Match>
    
    <Match>
        <Package name="com.imsweb.validation.internal" />
    </Match>
    
    <Match>
        <Package name="com.imsweb.validation.internal.context" />
    </Match>

    <Match>
        <Package name="com.imsweb.validation.entities.xml" />
    </Match>

    <Match>
        <Package name="com.imsweb.validation.entities.xml.set" />
    </Match>
    
    <Match>
        <Package name="com.imsweb.validation.entities.xml.tests" />
    </Match>

    <Match>
        <Class name="com.imsweb.validation.functions.MetafileContextFunctions" />
    </Match>

    <Match>
        <Bug category="MALICIOUS_CODE" />
    </Match>
    
    <Match>
        <Class name="com.imsweb.validation.entities.EditableRule" />
        <Bug pattern="SE_TRANSIENT_FIELD_NOT_RESTORED" />
    </Match>
</FindBugsFilter>